<% provide(:title, 'New') %>
<h1>New Hackday</h1>
<div class = "row" >
  <div class="span6 offset3">
    <%= form_for(@day) do |f| %>
      
      <%= f.label :title %>
      <%= f.text_field :title %>
            
      <%= f.submit "Create my Hackday", class: "btn btn-large btn-primary" %>
    
    <% end %>
  </div>
</div>